<FirstWindow>:
    name:"first"

    Image:
        source:"Background.png"
        allow_stretch: True
        keep_ratio:False
    BoxLayout:
        size_hint:0.93 , 0.8
        pos_hint:{'center_x':0.5, 'center_y':0.5}

        canvas.before:
            Color:
                rgba:1,1,1,0.3
            RoundedRectangle:
                pos:self.pos
                size:self.size
                radius:[20,]
    BoxLayout:
        size_hint:0.6,0.6
        pos_hint:{'center_x':0.30, 'center_y':0.7}

        Image: 
            source:"fsktm-logo.png"
            allow_stretch:False
            keep_ratio:True
            size:self.size

    Label:
        text:'SPECIALIZATION'
        font_size:60
        font_name : 'Kollektif-Bold.ttf'
        color : 0.953, 0.753, 0.075, 1
        halign : "right"
        pos_hint: {'center_x': 0.4, 'center_y':0.43}
        
    Label:
        text:'RECOMMENDATION'
        font_size:75
        font_name : 'Kollektif-Bold.ttf'
        color:0.251, 0.098, 0.561, 1
        halign : "right"
        pos_hint:{'center_x': 0.35, 'center_y':0.35}
    Label:
        text:'First time here?'
        font_size:43
        color:0.251, 0.098, 0.561, 1
        pos_hint: {'center_x':0.75, 'center_y': 0.6}
    Label:
        text:'Input your name'
        font_size:25
        color:0.251, 0.098, 0.561, 1
        pos_hint: {'center_x':0.75,'center_y':0.55}

    BoxLayout:
        size_hint:0.13,0.05
        pos_hint:{'center_x':0.75, 'center_y':0.5}

        canvas.before:
            Color:
                rgba:0.698, 0.737, 0.898, 1
            RoundedRectangle:
                pos:self.pos
                size:self.size
                radius:[20,]
        TextInput:
            id:user_name
            hint_text:'Your name...'
            hint_color : 1,1,1,1
            font_size:21
            background_color:0,0,0,0
            background_normal:''
            multiline:False
    
    Button:
        text:'Click here to start'
        size_hint:0.23, 0.07
        pos_hint: {'center_x': 0.75, 'center_y':0.4}
        background_color: 0, 0, 0, 0
        color: 0, 0, 0, 1 
        canvas.before:
            Color:
                rgba:1,1,1,1
            RoundedRectangle:
                pos:self.pos
                size:self.size
                radius:[30,]
        disabled: True if user_name.text == "" else False
        on_release:root.click_here_button()
        

